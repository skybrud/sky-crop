{
	"name": "sky-crop",
	"version": "1.0.15",
	"description": "Vue component for cropping images",
	"main": "dist/sky-crop.umd.js",
	"module": "dist/sky-crop.esm.js",
	"browser": {
		"./sfc": "src/SkyCrop.vue"
	},
	"scripts": {
		"dist": "rimraf dist && yarn run build:es && yarn run build:umd",
		"build:umd": "yarn rollup --config rollup.config.js --format umd --file dist/sky-crop.umd.js",
		"build:es": "yarn rollup --config rollup.config.js --format es --file dist/sky-crop.esm.js"
	},
	"author": "Skybrud.dk",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/skybrud/sky-crop.git"
	},
	"dependencies": {
		"lodash.throttle": "^4.1.1"
	},
	"devDependencies": {
		"sky-build-setup": "../sky-build-setup"
	},
	"peerDependencies": {
		"vue": "^2.5.0"
	}
}
