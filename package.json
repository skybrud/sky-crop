{
	"name": "sky-crop",
	"version": "1.0.15",
	"description": "Vue component for cropping images",
	"main": "dist/sky-crop.umd.js",
	"module": "dist/sky-crop.esm.js",
	"browser": {
		"./sfc": "src/sky-crop.vue"
	},
	"config": {
		"moduleName": "SkyCrop",
		"distName": "sky-crop"
	},
	"scripts": {
		"dist": "yarn run merge && yarn run build:es && yarn run build:umd",
		"build:umd": "yarn rollup --config rollup.config.js --format umd --file dist/sky-crop.umd.js",
		"build:es": "yarn rollup --config rollup.config.js --format es --file dist/sky-crop.esm.js",
		"merge": "./merge.sh"
	},
	"author": "Skybrud.dk",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/skybrud/sky-crop.git"
	},
	"dependencies": {
		"lodash.throttle": "^4.1.1"
	},
	"devDependencies": {
		"np": "3.0.4",
		"postcss": "^7.0.2",
		"rollup": "^0.57.1",
		"rollup-plugin-buble": "^0.19.2",
		"rollup-plugin-node-resolve": "^3.3.0",
		"rollup-plugin-vue": "3.0.0",
		"sky-build-setup": "../sky-build-setup",
		"vue": "^2.5.16",
		"vue-template-compiler": "^2.5.16"
	}
}
