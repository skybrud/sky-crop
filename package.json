{
  "name": "sky-crop",
  "version": "1.0.9",
  "description": "Vue component for cropping images",
  "main": "dist/skycrop.js",
  "scripts": {
    "build": "rimraf ./dist && webpack --config ./webpack.config.js",
    "dist": "yarn install && yarn run build",
    "update-build": "rimraf ./node_modules && yarn install",
    "release": "yarn run build && np --any-branch"
  },
  "build": "module",
  "author": "Skybrud.dk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skybrud/sky-crop.git"
  },
  "dependencies": {
    "sky-window": "^1.0.4"
  },
  "devDependencies": {
    "np": "*",
    "sky-build-setup": "https://github.com/skybrud/sky-build-setup.git#^1.0.0"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  }
}
